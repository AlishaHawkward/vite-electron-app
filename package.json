{
  "name": "vite-electron-app",
  "version": "0.0.0",
  "main": "dist/main/background.js",
  "description": "vite-electron-app",
  "homepage": "https://github.com/AlishaHawkward/vite-electron-app",
  "license": "MIT",
  "author": {
    "name": "YoumuKonpaku",
    "email": "kanade@acgme.cn"
  },
  "repository": {
    "url": "https://github.com/AlishaHawkward/vite-electron-app.git"
  },
  "build": {
    "appId": "com.youmukonpaku.example",
    "productName": "viteElectronApp",
    "copyright": "YoumuKonpaku",
    "files": [
      "!node_modules",
      "dist/main/**",
      "dist/render/**"
    ],
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "asar": false
  },
  "scripts": {
    "dev": "concurrently -k -n=react,electron -c=green,blue \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "NODE_ENV=development node build/electron.js",
    "build": "npm run build:react && npm run build:electron",
    "build:react": "vite build",
    "build:electron": "NODE_ENV=production node build/electron.js && electron-builder build",
    "serve:react": "vite preview",
    "clean": "rm -rf dist",
    "lint": "eslint src"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "concurrently": "^6.0.0",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "electron-connect": "^0.6.3",
    "esbuild": "^0.9.6",
    "eslint": "^7.22.0",
    "eslint-config-ali": "^12.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "less": "^4.1.1",
    "typescript": "^4.1.2",
    "vite": "^2.1.2",
    "vite-plugin-eslint": "^1.1.0"
  }
}
